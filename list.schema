include 'Config.groovy'

title "LAPPS LIST Request"
description "LIST request sent to a LAPPS DataSource to obtain a list of ID values."
type object

additionalProperties false
properties {
	discriminator {
		type string
		format 'uri'
	}
	payload {
		type object
		additionalProperties false
		properties {
			start { type integer }
			end { type integer }
		}
		required 'start', 'end'
	}
}
required 'discriminator'

