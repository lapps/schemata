include 'Config.groovy'

title "LAPPS LIST Request"
description "LIST request sent to LAPPS DataSource to obtain a list of artifact ID values."
type object

//dingleBerries 'Yes please'
type object
additionalProperties false
properties {
	discriminator {
		type string
		format 'uri'
	}
	token { $ref schema.token }
	payload {
		type object
		additionalProperties false
		properties {
			start { type integer }
			end { type integer }
		}
		required 'start', 'end'
	}
}
required 'discriminator'

